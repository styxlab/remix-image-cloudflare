{
  "name": "remix-image-cloudflare",
  "version": "1.0.0",
  "description": "Remix Image Component and Transformer Optimized for Cloudflare",
  "author": "Joost Jansky",
  "license": "MIT",
  "workspaces": [
    "examples/*"
  ],
  "sideEffects": false,
  "keywords": [],
  "type": "module",
  "main": "build/index.js",
  "exports": {
    ".": "./build/index.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "run-s build:module ex.build",
    "build:module": "tsc",
    "dev": "run-s dev:module ex.dev",
    "dev:module": "tsc",
    "test": "node test/tryout.js",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "clean": "rimraf ./build node_modules examples/build examples/.cache examples/node_modules",
    "typecheck": "tsc --noEmit",
    "ex.build": "yarn workspace basic build",
    "ex.dev": "yarn workspace basic dev",
    "ex.start": "yarn workspace basic start"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.2.0",
    "is-svg": "^4.3.2",
    "js-image-lib": "^0.1.7",
    "mime-tree": "^0.1.4",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.4.0",
    "@remix-run/eslint-config": "^1.3.5",
    "@types/eslint": "^8.4.1",
    "@types/node": "^17.0.23",
    "@types/react": "^18.0.3",
    "@types/react-dom": "^18.0.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.3",
    "yarn-run-all": "^3.1.1"
  },
  "engines": {
    "node": ">=14"
  },
  "packageManager": "yarn@3.2.0"
}
