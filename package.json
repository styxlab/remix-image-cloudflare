{
  "name": "remix-image-cloudflare",
  "version": "1.0.0",
  "description": "A React image component and on-demand image optimizer for Cloudflare",
  "author": "Joost Jansky",
  "license": "MIT",
  "sideEffects": false,
  "workspaces": [
    "examples/*"
  ],
  "keywords": [
    "react",
    "remix",
    "image",
    "responsive",
    "cloudflare"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/styxlab/remix-image-cloudflare.git"
  },
  "bugs": {
    "url": "https://github.com/styxlab/remix-image-cloudflare/issues"
  },
  "main": "build/index.js",
  "type": "module",
  "types": "build/index.d.ts",
  "exports": {
    ".": "./build/index.js",
    "./package.json": "./build/package.json",
    "./server": "./build/server.js"
  },
  "files": [
    "build",
    "package.json",
    "README.md",
    "server.d.ts"
  ],
  "scripts": {
    "build": "run-s build:module workers.build",
    "build:module": "rollup -c",
    "watch": "rollup -c -w",
    "dev": "run-s workers.dev",
    "start": "run-s workers.start",
    "test": "node test/tryout.js",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "clean": "rimraf ./build node_modules examples/build examples/.cache examples/node_modules",
    "typecheck": "tsc --noEmit",
    "workers.build": "yarn workspace cf-workers build",
    "workers.dev": "yarn workspace cf-workers dev",
    "workers.start": "yarn workspace cf-workers start",
    "deploy": "yarn workspace cf-workers deploy",
    "purge": "yarn workspace cf-workers purge"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.2.0",
    "is-svg": "^4.3.2",
    "js-image-lib": "^0.1.7",
    "mime-tree": "^0.1.4",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.4.0",
    "@remix-run/eslint-config": "^1.3.5",
    "@rollup/plugin-commonjs": "^21.0.3",
    "@rollup/plugin-inject": "^4.0.4",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.2.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@types/eslint": "^8.4.1",
    "@types/node": "^17.0.23",
    "@types/react": "^18.0.3",
    "@types/react-dom": "^18.0.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.1",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "typescript": "next",
    "yarn-run-all": "^3.1.1"
  },
  "engines": {
    "node": ">=14"
  },
  "packageManager": "yarn@3.2.0"
}
