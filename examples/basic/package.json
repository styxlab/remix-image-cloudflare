{
  "name": "basic",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "main": "build/index.js",
  "scripts": {
    "build": "remix build",
    "deploy": "yarn run build && wrangler publish",
    "dev": "run-p dev:remix dev:miniflare",
    "dev:remix": "remix watch",
    "dev:miniflare": "cross-env NODE_ENV=development miniflare ./build/index.js --watch",
    "postinstall": "remix setup cloudflare",
    "start": "cross-env NODE_ENV=production miniflare ./build/index.js",
    "typecheck": "tsc --project tsconfig.json --noEmit"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.2.0",
    "@remix-run/cloudflare-workers": "^1.3.5",
    "@remix-run/react": "1.3.5",
    "@remix-run/server-runtime": "^1.3.5",
    "cross-env": "^7.0.3",
    "query-string": "^7.1.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "remix-image-cloudflare": "*"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.4.0",
    "@cloudflare/wrangler": "^1.19.11",
    "@remix-run/dev": "1.3.5",
    "@remix-run/eslint-config": "1.3.5",
    "@types/react": "^18.0.3",
    "@types/react-dom": "^18.0.0",
    "eslint": "^8.13.0",
    "miniflare": "^2.4.0",
    "remix": "^1.3.5",
    "typescript": "^4.6.3",
    "yarn-run-all": "^3.1.1"
  },
  "engines": {
    "node": ">=14"
  },
  "resolutions": {
    "@cloudflare/kv-asset-handler": "^0.2.0"
  },
  "browser": {
    "./server.js": "./server.browser.js"
  }
}
